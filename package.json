  "main": "index.js",
  "scripts": {
    "start": "nodemon ./index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npx sequelize-cli db:seed:all",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --forceExit",
    "unittest": "cross-env NODE_ENV=unittest jest \".+([.]unit[.]test[.]js){1}\" --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "proxyquire": "^2.1.3",
    "sequelize-cli": "^6.3.0",
    "sequelize-mock": "^0.10.2",
    "supertest": "^6.1.6",
    "supertest-session": "^4.1.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-handlebars": "^5.3.3",
    "express-session": "^1.17.2",
    "mysql2": "^2.3.3-rc.0",
    "sequelize": "^6.9.0",
    "session-file-store": "^1.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules"
    ]
